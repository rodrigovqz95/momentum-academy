{
  "expo": {
    "name": "Momentum Academy",
    "slug": "momentum-academy",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon-ma.png",
    "splash": {
      "image": "./assets/momentum-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.momentumacademy",
      "buildNumber": "1.0.2",
      "infoPlist": {
        "NSUserActivityTypes": [
          "INSendMessageIntent",
          "INStartCallIntent"
        ]
      }
    },
    "android": {
      "package": "com.momentumacademy",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2,
      "permissions": [
        "NOTIFICATIONS"
      ],
      "useNextNotificationsApi": true,
      "icon": "./assets/android-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
