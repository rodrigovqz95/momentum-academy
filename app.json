{
  "expo": {
    "name": "Momentum Academy",
    "slug": "momentum-academy",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon-ma.png",
    "splash": {
      "image": "./assets/splash-ma-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.momentumacademy",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.momentumacademy",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS"
      ],
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
